<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical">

    <LinearLayout
        android:id="@+id/ll_non_acs"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/item_normal_margin"
        android:orientation="vertical"
        android:visibility="visible">

        <TextView
            style="@style/tv_normal_titel"
            android:layout_width="match_parent"
            android:layout_height="@dimen/item_normal_height"
            android:gravity="center_vertical"
            android:text="非ACS心源性胸痛类型" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <CheckBox
                    android:id="@+id/cb_non_acs_5"
                    style="@style/XyjCheckBoxHorizontalWeight"
                    android:text="心肌炎" />

                <CheckBox
                    android:id="@+id/cb_non_acs_6"
                    style="@style/XyjCheckBoxHorizontalWeight"
                    android:text="冠心病" />

                <CheckBox
                    android:id="@+id/cb_non_acs_7"
                    style="@style/XyjCheckBoxHorizontalWeight"
                    android:text="瓣膜性心脏病" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <CheckBox
                    android:id="@+id/cb_non_acs_8"
                    style="@style/XyjCheckBoxHorizontalWeight"
                    android:text="陈旧性心肌梗死" />

                <CheckBox
                    android:id="@+id/cb_non_acs_9"
                    style="@style/XyjCheckBoxHorizontalWeight"
                    android:text="心绞痛" />

                <CheckBox
                    android:id="@+id/cb_non_acs_24"
                    style="@style/XyjCheckBoxHorizontalWeight"
                    android:text="张力性气胸" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <CheckBox
                    android:id="@+id/cb_non_acs_10"
                    style="@style/XyjCheckBoxHorizontalWeight"
                    android:text="心悸" />

                <CheckBox
                    android:id="@+id/cb_non_acs_11"
                    style="@style/XyjCheckBoxHorizontalWeight"
                    android:text="房颤" />

                <CheckBox
                    android:id="@+id/cb_non_acs_12"
                    style="@style/XyjCheckBoxHorizontalWeight"
                    android:text="高血压" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <CheckBox
                    android:id="@+id/cb_non_acs_13"
                    style="@style/XyjCheckBoxHorizontalWeight"
                    android:text="心衰" />

                <CheckBox
                    android:id="@+id/cb_non_acs_14"
                    style="@style/XyjCheckBoxHorizontalWeight"
                    android:text="房扑" />

                <CheckBox
                    android:id="@+id/cb_non_acs_15"
                    style="@style/XyjCheckBoxHorizontalWeight"
                    android:text="室早" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <CheckBox
                    android:id="@+id/cb_non_acs_16"
                    style="@style/XyjCheckBoxHorizontalWeight"
                    android:text="房早" />

                <CheckBox
                    android:id="@+id/cb_non_acs_17"
                    style="@style/XyjCheckBoxHorizontalWeight"
                    android:text="室上速" />

                <CheckBox
                    android:id="@+id/cb_non_acs_18"
                    style="@style/XyjCheckBoxHorizontalWeight"
                    android:text="心包炎" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <CheckBox
                    android:id="@+id/cb_non_acs_19"
                    style="@style/XyjCheckBoxHorizontalWeight"
                    android:text="心包积液" />

                <CheckBox
                    android:id="@+id/cb_non_acs_20"
                    style="@style/XyjCheckBoxHorizontalWeight"
                    android:text="心肌桥" />

                <CheckBox
                    android:id="@+id/cb_non_acs_23"
                    style="@style/XyjCheckBoxHorizontalWeight"
                    android:text="主动脉瘤" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/item_normal_height"
                android:orientation="horizontal">

                <CheckBox
                    android:id="@+id/cb_non_acs_1"
                    style="@style/XyjCheckBoxHorizontalWeight"
                    android:text="心律失常" />

                <CheckBox
                    android:id="@+id/cb_non_acs_2"
                    style="@style/XyjCheckBoxHorizontalWeight"
                    android:text="扩张性心肌病" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <CheckBox
                    android:id="@+id/cb_non_acs_3"
                    style="@style/XyjCheckBoxHorizontalWeight"
                    android:text="缺血性心肌病" />

                <CheckBox
                    android:id="@+id/cb_non_acs_4"
                    style="@style/XyjCheckBoxHorizontalWeight"
                    android:text="肥厚性心肌病" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <CheckBox
                    android:id="@+id/cb_non_acs_21"
                    style="@style/XyjCheckBoxHorizontalWeight"
                    android:text="主动脉观壁间血肿" />

                <CheckBox
                    android:id="@+id/cb_non_acs_22"
                    style="@style/XyjCheckBoxHorizontalWeight"
                    android:text="透壁性动脉粥样硬化性溃疡" />

            </LinearLayout>


        </LinearLayout>
    </LinearLayout>

</LinearLayout>