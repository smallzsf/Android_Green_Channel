<?xml version="1.0" encoding="utf-8"?>
<resources>

    <attr name="topLineVisible" format="boolean" />
    <attr name="bottomLineVisible" format="boolean" />
    <attr name="title" format="string|reference" />

    <declare-styleable name="BaseTitleBar">

        <attr name="title" />
        <attr name="titleSize" format="dimension" />
        <attr name="titleColor" format="color" />
        <attr name="btb_closeIconVisible" format="boolean" />
        <attr name="leftIcon" format="reference" />
        <attr name="leftIconVisible" format="boolean" />
        <attr name="titleBackground" format="color" />
        <attr name="rightIcon" format="reference" />
        <attr name="rightIconVisible" format="boolean" />
        <attr name="rightText" format="string" />
        <attr name="rightTextVisible" format="boolean" />
        <attr name="bottomLineVisible" />

    </declare-styleable>

    <declare-styleable name="EditSpinner">
        <attr name="hint" format="string" />
        <attr name="rightImage" format="reference" />
        <attr name="Background" format="reference" />
    </declare-styleable>

    <declare-styleable name="TextSwitchBar">

        <attr name="tsb_title" format="string|reference" />
        <attr name="tsb_switch" format="boolean" />

    </declare-styleable>

    <declare-styleable name="TextTimeBar">

        <attr name="title" />
        <attr name="topLineVisible" />
        <attr name="bottomLineVisible" />
        <attr name="ttb_time_hint" format="string|reference" />
        <attr name="ttb_time" format="string|reference" />
        <attr name="ttb_edit_enable" format="boolean" />

    </declare-styleable>
    <declare-styleable name="NihssItemBar">
        <attr name="title" />
        <attr name="nib_score_visible" format="boolean" />
        <attr name="nib_title_size" format="dimension" />
    </declare-styleable>

    <declare-styleable name="ActionSheet">
        <attr name="actionSheetBackground" format="color|reference" />
        <attr name="cancelButtonBackground" format="color|reference" />
        <attr name="otherButtonTopBackground" format="color|reference" />
        <attr name="otherButtonMiddleBackground" format="color|reference" />
        <attr name="otherButtonBottomBackground" format="color|reference" />
        <attr name="otherButtonSingleBackground" format="color|reference" />
        <attr name="cancelButtonTextColor" format="color|reference" />
        <attr name="otherButtonTextColor" format="color|reference" />
        <attr name="actionSheetPadding" format="dimension|reference" />
        <attr name="otherButtonSpacing" format="dimension|reference" />
        <attr name="cancelButtonMarginTop" format="dimension|reference" />
        <attr name="actionSheetTextSize" format="dimension|reference" />
    </declare-styleable>

    <declare-styleable name="ActionSheets">
        <attr name="actionSheetStyle" format="reference" />
    </declare-styleable>
</resources>